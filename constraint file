set_property -dict { PACKAGE_PIN N11    IOSTANDARD LVCMOS33 } [get_ports { clk }];
set_property -dict { PACKAGE_PIN J3    IOSTANDARD LVCMOS33 } [get_ports { light[0] }]; #green
set_property -dict { PACKAGE_PIN H3    IOSTANDARD LVCMOS33 } [get_ports { light[1] }]; #yellow
set_property -dict { PACKAGE_PIN J1    IOSTANDARD LVCMOS33 } [get_ports { light[2] }]; #red
set_property -dict { PACKAGE_PIN T9    IOSTANDARD LVCMOS33 } [get_ports { clk_out }];  #clk_out
